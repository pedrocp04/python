# -*- coding: utf-8 -*-

n1 = int(input("Digite o primeiro número: ")
n2 = int("Digite o segundo número: ")
sinal = input("Digite o sinal: ")

if sinal == "+":
  op = n1 + n2
elif sinal == "-"
  op =  n1 - n2 
elif sinal == "/":
  op = n1/n2
elif sinal == "*":
  op = n1*n2
else:
  print("Sinal invalido")

print(op)
